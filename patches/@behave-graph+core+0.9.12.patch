+export const ForLoop = makeFlowNodeDefinition({
+    typeName: 'flow/forLoop',
+    category: NodeCategory.Flow,
+    label: 'For Loop',
+    in: {
+        flow: 'flow',
+        startIndex: 'integer',
+        endIndex: 'integer'
