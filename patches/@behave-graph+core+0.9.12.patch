+    out: {
+        loopBody: 'flow',
+        index: 'integer',
+        completed: 'flow'
+    },
+    initialState: undefined,
+    triggered: ({ read, write, commit }) => {
+        const startIndex = read('startIndex');
+        const endIndex = read('endIndex');
+        const loopBodyIteration = (i) => {
+            if (i < endIndex) {
+                write('index', i);
+                commit('loopBody', () => {
+                    loopBodyIteration(i + BigInt(1));
+                });
+            }
+            else {
+                commit('completed');
+            }
+        };
+        loopBodyIteration(startIndex);
+    }
