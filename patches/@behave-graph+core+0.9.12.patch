+    node.metadata = nodeJson?.metadata ?? node.metadata;
+    if (nodeJson.parameters !== undefined) {
+        readNodeParameterJSON(registry.values, node, nodeJson.parameters);
+    }
+    if (nodeJson.flows !== undefined) {
