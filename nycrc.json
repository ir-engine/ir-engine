{
  "extends": "@istanbuljs/nyc-config-typescript",
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    ".d.ts",
    ".test.ts",
    ".test.tsx"
  ],
  "check-coverage": true,
  "per-file": true,
  "statements": 80,
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "watermarks": {
    "statements": [
      80,
      95
    ],
    "branches": [
      80,
      95
    ],
    "functions": [
      80,
      95
    ],
    "lines": [
      80,
      95
    ]
  }
}

