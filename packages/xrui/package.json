{
  "name": "@ir-engine/xrui",
  "version": "1.6.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/ecs.cjs",
  "module": "./dist/ecs.js",
  "exports": {
    ".": {
      "types": "./dist/ecs.d.ts",
      "import": "./dist/ecs.js",
      "default": "./dist/ecs.js",
      "require": "./dist/ecs.cjs"
    }
  },
  "description": "A set of utilities for using DOM elements to create immerive user interfaces",
  "homepage": "https://github.com/ir-engine/ir-engine#readme",
  "keywords": [
    "3D",
    "AR",
    "DOM",
    "VR",
    "XR",
    "three",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ir-engine/ir-engine.git"
  },
  "author": {
    "name": "Infinite Reality Engine Collective",
    "email": "hello@ir-engine.org",
    "url": "http://github.com/ir-engine"
  },
  "bugs": {
    "url": "https://github.com/ir-engine/ir-engine/issues"
  },
  "engines": {
    "node": ">= 18.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmClient": "npm",
  "scripts": {
    "check-errors": "tsc --noemit",
    "build": "tsc && rimraf dist && rollup -c",
    "build:worker": "esbuild src/textures/KTX2Worker.ts --bundle --format=esm --outfile=src/textures/KTX2Worker.bundle.js --loader:.wasm=binary",
    "test": "cross-env TEST=true mocha --config .mocharc.cjs"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "dexie": "^3.2.3",
    "fflate": "^0.7.4",
    "msgpackr": "^1.8.5"
  },
  "devDependencies": {
    "@types/node": "18.15.5",
    "cross-env": "^7.0.3",
    "esbuild": "0.17.12",
    "rimraf": "4.4.0",
    "rollup": "^4.20.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.4.5"
  },
  "license": "CPAL"
}